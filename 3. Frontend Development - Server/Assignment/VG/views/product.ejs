<h1>Products</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Price</th>
    <th>Quantity</th>
    <th>Category</th>
  </tr>

  <% products.forEach((product) => { %>
  <tr>
    <td><%= product.name %></td>
    <td><%= product.description %></td>
    <td><%= product.price %></td>
    <td><%= product.quantity %></td>
    <td><%= product.category %></td>
  </tr>
  <% }); %>
</table>